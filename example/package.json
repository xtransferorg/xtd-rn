    "open:release-apk": "open ./android/app/build/outputs/apk/release",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "patch-package && xrn-bundle-postinstall",
    "bind-port:harmony": "hdc rport tcp:8101 tcp:8101"
